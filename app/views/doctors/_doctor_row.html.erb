<tr id="doctorRow_<%= doctor.id %>">
 <td><%= doctor.name %></td>
  <td><%= doctor.fname %></td>
  <td><%= doctor.email %></td>
  <td><%= doctor.phone %></td>
  <td><%= link_to 'Show', doctor %></td>
  <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
